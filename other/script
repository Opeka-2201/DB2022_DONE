CREATE TABLE Tache(
   ID      INT NOT NULL AUTO_INCREMENT PRIMARY KEY
  ,EMPLOYE INTEGER  NOT NULL 
  ,PROJET  VARCHAR(64) NOT NULL
  ,TITRE   VARCHAR(64)
);
