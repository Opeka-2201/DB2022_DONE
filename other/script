CREATE TABLE Employe(
   NO              INTEGER  NOT NULL PRIMARY KEY 
  ,NOM             VARCHAR(64) NOT NULL
  ,NOM_DEPARTEMENT VARCHAR(10)
  ,NOM_FONCTION    VARCHAR(128)
);
