CREATE DATABASE group25;
USE group25;
CREATE TABLE Departement(
   NOM    VARCHAR(10) NOT NULL PRIMARY KEY
  ,BUDGET INTEGER 
);
INSERT INTO Departement(NOM,BUDGET) VALUES ('INF',50000);
INSERT INTO Departement(NOM,BUDGET) VALUES ('MATH',NULL);
